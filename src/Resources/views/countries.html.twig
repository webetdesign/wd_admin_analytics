<div class="Chartjs p-4 shadow bg-block-analytics box box-primary" >
    <div class="box-header box-header-analytics">
        {% if icon is defined %}
            <i class="{{ icon }}"></i>
        {% endif %}
        <h3 class="box-title ">Pays </h3>
    </div>
    <div class="box-body">
        <div id="countriesMap-container" data-start="{{ start }}" style="width: 100%; height: 300px; display: none">
            Chargement...
        </div>
        <figure class="Chartjs-figure" data-start="{{ start }}" id="countriesChart-container"></figure>
        <ol class="Chartjs-legend" id="countriesChart-legend"></ol>
    </div>
</div>
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
