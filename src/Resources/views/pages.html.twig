<div class="Chartjs p-4 shadow bg-block-analytics box box-primary" >
    <div class="box-header box-header-analytics">
        {% if icon is defined %}
            <i class="{{ icon }}"></i>
        {% endif %}
        <h3 class="box-title ">Pages </h3>
    </div>
    <div class="box-body">
        <div class="Chartjs-figure" id="pages-container"></div>
        <ol class="Chartjs-legend" id="pages-legend"></ol>
    </div>
</div>
